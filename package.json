{
  "name": "sdh-gamesync",
  "version": "1.0.1",
  "description": "A fork of Decky Cloud Save that manages cloud saves and mods for games",
  "packageManager": "pnpm@10.12.3",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkazaRenn/SDH-GameSync.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "plugin-template",
    "steam-deck",
    "deck",
    "cloud",
    "save",
    "backup"
  ],
  "author": "Akaza Renn",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/AkazaRenn/SDH-GameSync/issues"
  },
  "homepage": "https://github.com/AkazaRenn/SDH-GameSync#readme",
  "devDependencies": {
    "@decky/rollup": "^1.0.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/webpack": "^5.28.5",
    "rollup": "^4.44.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@decky/api": "^1.1.2",
    "@decky/ui": "^4.10.4",
    "fastq": "^1.19.1",
    "react-icons": "^5.5.0",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
